{
  "name": "Social",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "yarn android-simulator",
    "ios": "react-native run-ios",
    "start": "env-cmd -f ./.env react-native start --reset-cache",
    "android-build": "cd android && ./gradlew assembleDemo",
    "android-bundle": "cd android && ./gradlew bundleDemo",
    "android-simulator": "env-cmd -f ./.env react-native run-android",
    "android-simulator-prod": "env-cmd -f ./.env react-native run-android --variant=demoRelease",
    "android-device": "yarn android-simulator",
    "ios-simulator": "env-cmd -f ./.env react-native run-ios --simulator=\"iPhone X\"",
    "ios-device": "env-cmd -f ./.env react-native run-ios --device",
    "ios-device-prod": "env-cmd -f ./.env react-native run-ios --device --configuration=Release",
    "test": "jest",
    "fastlane": "cd ios && fastlane",
    "extract-fragment-types": "env-cmd -f ./.env node src/utils/extract-fragment-types.js",
    "demo-debug": "cd android && ./gradlew assembleDemoDebug",
    "demo-release": "cd android && ./gradlew assembleDemoRelease",
    "staging-debug": "cd android && ./gradlew assembleStagingDebug",
    "staging-release": "cd android && ./gradlew assembleStagingRelease",
    "full-debug": "cd android && ./gradlew assembleFullDebug",
    "full-release": "cd android && ./gradlew assembleFullRelease",
    "postinstall": "jetifier -r",
    "lint": "tslint -p \".\" -t stylish"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-community/image-editor": "^2.3.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^4.6.1",
    "@react-native-community/viewpager": "^3.1.0",
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.36",
    "@types/react-intl": "^3.0.0",
    "@types/react-native": "^0.62.13",
    "@types/react-native-vector-icons": "^6.4.4",
    "@types/react-navigation": "^3.0.8",
    "@types/react-test-renderer": "^16.8.2",
    "@types/yup": "^0.29.9",
    "bottlejs": "^2.0.0",
    "env-cmd": "^10.1.0",
    "events": "^3.2.0",
    "intl": "^1.2.5",
    "native-base": "^2.13.8",
    "process": "^0.11.10",
    "prop-types": "^15.6.2",
    "react": "16.13.1",
    "react-intl": "2.9.0",
    "react-native": "0.63.3",
    "react-native-eject": "^0.1.2",
    "react-native-elements": "^3.0.0-alpha.1",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-reanimated": "^1.13.1",
    "react-native-safe-area-context": "^3.1.8",
    "react-native-screens": "^2.12.0",
    "react-native-step-indicator": "^1.0.3",
    "react-native-tab-view": "^2.15.2",
    "react-navigation": "^4.4.3",
    "react-navigation-drawer": "^2.6.0",
    "react-navigation-stack": "^2.10.0",
    "react-navigation-tabs": "^2.10.1",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/runtime": "^7.12.1",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-cli": "^6.26.0",
    "babel-jest": "^26.6.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-plugin-transform-remove-console": "^6.8.1",
    "babel-preset-react-native": "1.9.1",
    "bottlejs": "^2.0.0",
    "eslint": "^7.12.1",
    "jest": "^26.6.1",
    "jetifier": "^1.6.6",
    "metro-react-native-babel-preset": "^0.63.0",
    "react-native-dotenv": "^2.4.2",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
